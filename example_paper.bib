issn = {07351097},
journal = {Journal of the American College of Cardiology},
keywords = {ablation,atrial fibrillation,electrical rotors,focal beats,multiwavelet reentry,therapy},
month = {aug},
number = {7},
pages = {628--636},
title = {{Treatment of atrial fibrillation by the ablation of localized sources: CONFIRM (Conventional Ablation for Atrial Fibrillation with or Without Focal Impulse and Rotor Modulation) trial}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22818076 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3416917},
