issn = {15383598},
month = {apr},
number = {13},
pages = {1261--1274},
publisher = {American Medical Association},
title = {{Effect of Catheter Ablation vs Antiarrhythmic Drug Therapy on Mortality, Stroke, Bleeding, and Cardiac Arrest among Patients with Atrial Fibrillation: The CABANA Randomized Clinical Trial}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/30874766 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6450284},
volume = {321},
